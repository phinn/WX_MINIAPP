.tui-list-box{
  border-bottom: 1px solid #c8c7cc;
  background-color: #fff;
}
.tui-list-head{
  background-color: #efeff4;
  line-height: 80rpx;
  color: #6d6d72;
  padding: 0 10px;
  font-size: 35rpx;
  border-bottom: 1px solid #c8c7cc;
}
.tui-list-box-raduis{
  border: 1px solid #c8c7cc;
  border-radius: 4px;
  margin: 10px;
  overflow: hidden;
  background-color: #fff;
}
.list-h-line{
  display: flex;
}
.list-text{
  margin-top:10px;
}
.list-img {
  width: 50px;
  height:50px;
  margin:10px;
}
.list-img image {
  width: 50px;
  height:50px;
}